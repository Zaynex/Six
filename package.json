{
  "name": "six",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:server": "tsc -w -p server",
    "server": "nodemon dist/server/app.js",
    "start": "ng serve --proxy-config proxy.conf.json -o",
    "dev": "concurrently \"mongod\"  \"ng serve --proxy-config proxy.conf.json -o\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --prod\" \"tsc -w -p server\" \"node dist/server/app.js\" ",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "mongoose": "^5.1.2",
    "morgan": "^1.9.0",
    "rxjs": "^6.0.0",
    "serve-favicon": "^2.5.0",
    "zone.js": "^0.8.26",
    "@angular/cdk": "^6.1.0"
  },
